<script>
$(document).ready(function() {
    function updateMinicart() {
        $.ajax({
            url: '/get-minicart',
            type: 'GET',
            success: function(html) {
                $('#minicart-container').html(html);
            },
            error: function(error) {
                console.error('Error updating minicart:', error);
            }
        });
    }

    function updateCartCount(count) {
        $('#CartCount').text(count);
    }



    $('.btn-addto-cart').on('click', function(event) {
        event.preventDefault();
        var productId = $(this).closest('form').find('input[name="id"]').val();
        var isLoggedIn = {{#if user}}true{{else}}false{{/if}};

        if (isLoggedIn) {
            $.ajax({
                url: `/add-to-cart?productId=${productId}`,
                type: 'POST',
                success: function(response) {
                    if (response.success) {
                        document.dispatchEvent(new Event('productAdded'));
                        updateMinicart();
                        updateCartCount(response.itemCount);

                        $('#cart-popup').fadeIn();
                        setTimeout(function() {
                            $('#cart-popup').fadeOut();
                        }, 2000);
                    } else {

                    }
                },
                error: function() {
                    Swal.fire({
                        icon: 'error',
                        title: 'Internal Server Error',
                        text: 'Something went wrong on the server.',
                    });
                }
            });
        } else {
            var guestCart = JSON.parse(localStorage.getItem('guestCart')) || [];
            var itemIndex = guestCart.findIndex(item => item.productId === productId);

            if (itemIndex > -1) {
                guestCart[itemIndex].quantity += 1;
            } else {
                guestCart.push({ productId: productId, quantity: 1 });
            }

            localStorage.setItem('guestCart', JSON.stringify(guestCart));
            guestCart = JSON.parse(localStorage.getItem('guestCart'));


            $.ajax({
                url: '/add-to-cart',
                type: 'POST',
                contentType: 'application/json',
                data: JSON.stringify({ guestCart: guestCart }),
                success: function(response) {
                    if (response.success) {
                        document.dispatchEvent(new Event('productAdded'));
                        updateMinicart();
                        updateCartCount(response.itemCount);

                        if(response.clearGuestCart){
                            localStorage.removeItem('guestCart');
                        }
                        $('#cart-popup').fadeIn();
                        setTimeout(function() {
                            $('#cart-popup').fadeOut();
                        }, 2000);

                    } else {

                    }
                },
                error: function() {
                    Swal.fire({
                        icon: 'error',
                        title: 'Internal Server Error',
                        text: 'Something went wrong on the server.',
                    });
                }
            });
        }
    });

    // Add event listener for removing items from cart
    $(document).on('click', '.remove-from-cart', function(event) {
        event.preventDefault();
        var productId = $(this).data('product-id');

        $.ajax({
            url: `/remove-from-cart/${productId}`,
            type: 'POST',
            success: function(response) {
                if (response.success) {
                    updateMinicart();
                    updateCartCount(response.itemCount);
                } else {

                }
            },
            error: function() {
                Swal.fire({
                    icon: 'error',
                    title: 'Internal Server Error',
                    text: 'Something went wrong on the server.',
                });
            }
        });
    });
});
</script>


