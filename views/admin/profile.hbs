   <script>
        function validateForm() {
            let username = document.forms["adminForm"]["username"].value;
            let email = document.forms["adminForm"]["email"].value;
            let password = document.forms["adminForm"]["password"].value;
            let confirmPassword = document.forms["adminForm"]["confirmpassword"].value;

            if (!username.trim()|| !email.trim() || !password.trim() || !confirmPassword.trim()) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Please fill all the fields.',
                });
                return false;
            }
            if (username.length < 3 || username.length > 20) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Username must be between 3 and 20 characters.',
                });
                return false;
            }
            let passwordPattern = /^(?=.*[a-z])(?=.*[A-Z])(?=.*\d)(?=.*[\W_]).+$/;
            if (!passwordPattern.test(password)) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Password should contain 1 uppercase letter, 1 lowercase letter, 1 symbol, and 1 number.',
                });
                return false;
            }

            if (password !== confirmPassword) {
                Swal.fire({
                    icon: 'warning',
                    title: 'Passwords do not match.',
                });
                return false;
            }

            return true;
        }
    </script>
