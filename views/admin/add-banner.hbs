            <div class="card">
                <div class="card-body">
                    <div class="card-title">Add Banner</div>
                    <hr>
                    <form action="/add-banner" method="post" enctype="multipart/form-data">
                        <div class="form-group">
                            <label for="name">Title</label>
                            <input type="text" class="form-control" name="title" id="title" placeholder="Enter Banner Title" >
                        </div>
                        <div class="form-group">
                            <label for="input-6">Banner Image</label>
                            <div class="input-group">
                                
                                <div class="custom-file">
                                    <input type="file" id="Image" name="Image" class="custom-file-input" accept="image/*" aria-describedby="input-6-addon" multiple>
                                    <label class="custom-file-label" for="input-6">Choose file</label>
                                </div>
                            </div>
                        </div>
                        <p class="text-danger">{{error}}</p>
                        <div class="form-group">
                            <button type="submit" class="btn btn-light px-5"> Add Banner</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
